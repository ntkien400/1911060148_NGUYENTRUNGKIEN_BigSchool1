@model _1911060148_NGUYENTRUNGKIEN_BigSchool1.ViewModels.FollowingsViewModel
@{
    ViewBag.Title = "FollowingMeList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Danh sách người theo dõi tôi</h2>

<table class="follow" style="width:15%">
    <tr>
        <th>Người theo dõi</th>
    </tr>
    @foreach (var lecturers in Model.FollowingLecturers)
    {
        <tr>
            <td>
                <div class="details">
                    <span class="lecturer">
                        @lecturers.Name
                    </span>
                </div>
            </td>
        </tr>
    }
</table>
